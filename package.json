{
    "name": "@surfboard/sources",
    "version": "0.2.0",
    "license": "MIT",
    "description": "Provides ready-to-use observable sources for surfboard projects",
    "repository": {
        "type": "git",
        "url": "https://github.com/adaedra/surfboard-sources.git"
    },
    "homepage": "https://github.com/adaedra/surfboard-sources",
    "main": "./dist/index.js",
    "devDependencies": {
        "@types/jest": "^23.3.12",
        "@types/lolex": "^2.1.3",
        "@types/node": "^10.12.18",
        "@types/node-fetch": "^2.1.4",
        "@types/node-schedule": "^1.2.2",
        "jest": "^23.6.0",
        "lolex": "^3.0.0",
        "ts-jest": "^23.10.5",
        "typescript": "^3.2.2"
    },
    "dependencies": {
        "node-fetch": "^2.3.0",
        "node-schedule": "^1.3.1",
        "rxjs": "^6.3.3"
    },
    "scripts": {
        "test": "jest",
        "build": "tsc -p tsconfig.build.json",
        "prepare": "npm run build"
    },
    "jest": {
        "transform": {
            "\\.ts$": "ts-jest"
        },
        "testRegex": "/.*\\.test\\.ts$",
        "moduleFileExtensions": [
            "ts",
            "js",
            "json",
            "node"
        ]
    }
